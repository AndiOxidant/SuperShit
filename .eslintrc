{
    extends: "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "node": true,
        "mocha": true,
        "es6": true
    },
    "rules": {
        "no-console": 1,
        "no-unused-vars": [2, {"vars": "all", "args": "none"}]
    }
}
